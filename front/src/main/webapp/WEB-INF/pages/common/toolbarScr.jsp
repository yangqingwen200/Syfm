<%@ page language="java" contentType="text/html; charset=utf-8" %>

<%-- bootstrap table toolbar工具栏公用的搜索 重置 刷新按钮 --%>
<div style="float: right;margin-bottom: 5px">
    <button id="toolbar_search_table" type="button" class="btn btn-primary btn-sm">
        <span class="glyphicon glyphicon-search" aria-hidden="true"></span> 查询
    </button>
    <button id="toolbar_clear_table" type="button" class="btn btn-default btn-sm">
        <span class="glyphicon glyphicon-remove-circle" aria-hidden="true"></span> 重置
    </button>
    <button id="toolbar_refresh_table" type="button" class="btn btn-default btn-sm">
        <span class="glyphicon glyphicon-refresh" aria-hidden="true"></span> 刷新
    </button>
</div>
